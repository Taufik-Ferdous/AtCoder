#include<bits/stdc++.h>
using namespace std;

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

#define ll long long

//====================================

void solve(){
	string s; cin>>s;
	if(islower(s[0])){
		cout<<"No" <<'\n';
		return;
	}
	for(ll i=1; i<s.length(); ++i){
		if(isupper(s[i])){
			cout<<"No" <<'\n';
			return;
		}
	}
	cout<<"Yes" <<'\n';
}


int main(){
	optimize();

	ll t=1;
	// cin>>t;
	while(t--)
		solve();
}
