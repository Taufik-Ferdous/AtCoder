#include<bits/stdc++.h>
using namespace std;

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

#define ll long long

//====================================

void solve(){
	ll n; cin>>n;
	ll a=0, b=0;
	while(n--){
		ll c,d; cin>>c >>d;
		a+=c;
		b+=d;
	}
	if(a>b) cout<<"Takahashi" <<'\n';
	else if(a<b) cout<<"Aoki" <<'\n';
	else cout<<"Draw" <<'\n';
}


int main(){
	optimize();

	ll t=1;
	// cin>>t;
	while(t--)
		solve();
}
